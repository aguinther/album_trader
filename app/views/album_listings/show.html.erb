<div class="page-header">
  <%= link_to album_listings_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_album_listing_path(@album_listing), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show album_listing</h1>
</div>

<dl class="dl-horizontal">
  <dt>Price:</dt>
  <dd><%= @album_listing.price %></dd>

  <dt>Quantity:</dt>
  <dd><%= @album_listing.quantity %></dd>

  <dt>Condition:</dt>
  <dd><%= @album_listing.condition %></dd>

</dl>
